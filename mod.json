{
	"geode": "4.0.0-alpha.1",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074"
	},
	"version": "v1.3.0",
	"id": "techstudent10.button_sfx",
	"name": "Button SFX",
	"developer": "TechStudent10",
	"description": "Plays SFX whenever a button is clicked!",
	"links": {
		"source": "https://github.com/TechStudent10/HideLocks"
	},
	"settings": {
		"sfx": {
			"type": "file",
			"name": "SFX",
			"description": "The SFX sound file",
			"default": "",
			"control": {
				"filters": [
					{
						"description": "Sound Files",
						"files": [
							"mp3",
							"ogg"
						]
					}
				]
			}
		},
		"in-editor": {
			"type": "bool",
			"default": false,
			"description": "Should play in the editor",
			"name": "Play in editor"
		},
		"volume": {
			"type": "float",
			"default": 1,
			"description": "The volume level of the sound effect",
			"name": "Volume",
			"control": {
				"slider": true
			},
			"max": 1,
			"min": 0
		}
	}
}
