{
	"geode": "3.1.1",
	"gd": {
		"win": "2.206",
		"android": "2.206",
		"mac": "2.206"
	},
	"version": "v1.2.0",
	"id": "techstudent10.button_sfx",
	"name": "Button SFX",
	"developer": "TechStudent10",
	"description": "Plays a sound effect whenever a button is clicked!",
	"settings": {
		"sfx": {
			"type": "path",
			"name": "SFX",
			"description": "The SFX sound file",
			"default": "",
			"control": {
				"filters": [
					{
						"description": "Sound Files",
						"files": [
							"mp3",
							"ogg"
						]
					}
				]
			}
		},
		"in-editor": {
			"type": "bool",
			"default": false,
			"description": "Should play in the editor",
			"name": "Play in editor"
		},
		"volume": {
			"type": "float",
			"default": 1,
			"description": "The volume level of the sound effect",
			"name": "Volume",
			"control": {
				"slider": true
			},
			"max": 1,
			"min": 0
		}
	}
}
